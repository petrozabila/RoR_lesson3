<!DOCTYPE html>
<html>
<head>
  <title>TestApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<%= render 'layouts/header' %>

<div id="nav">
	<ul style="list-style-type:none">
	<li><%= link_to 'Login', sessions_login_path %></li><br>
	<li><%= link_to 'Logout', sessions_logout_path %></li><br>
	<li><%= link_to 'Registration', registration_path %></li><br>
</ul>
</div>

<div id="section">
</div>
	
	<%= current_user.name if current_user%>

	<%= yield %>

	<%= render 'layouts/footer' %>

</body>
</html>
